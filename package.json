{
  "name": "crystal-event-emitter",
  "version": "2.1.3",
  "description": "A small, modern event emitter base class with no dependencies written in ES2015+",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "registry": "npm",
  "keywords": [
    "event",
    "emitter",
    "crystal",
    "esnext",
    "es2015",
    "es2016",
    "es2017",
    "es6",
    "modern"
  ],
  "scripts": {
    "check-commit": "npm test && npm run lint",
    "compile": "babel src -d dist",
    "coverage": "nyc npm test",
    "lint": "eslint src",
    "test": "ava",
    "prepublishOnly": "npm run check-commit"
  },
  "author": "kdex",
  "license": "GPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "codecov": "^3.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.16.0",
    "eslint-plugin-filenames": "^1.2.0",
    "greenkeeper-lockfile": "^1.13.2",
    "nyc": "^11.4.1"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  }
